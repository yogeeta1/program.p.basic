#include<stdio.h>
#include<conio.h>
int main()
{
    int a;  
    printf("Enter a number from 1 to 5 :\n");
    scanf("%d",&a);  
    switch(a)
    {
    case 1 : {
    	printf("Food item - Pizza \n price - Rs 239\n");
		break;
	}
   
    case 2 :{printf("Food item - Cachori \n price -Rs 129\n");
		break;
	} 
    case 3 :{printf("Food item - vada pav \nprice -Rs 179\n");
		break;
	} 
    
    case 4 : {printf("Food item - burger \n price -Rs 99\n");
		break;
	}
    
    case 5 :{ printf("Food item - Masala dosa\n price -Rs 149\n");
		break;
	}
    default:printf("\n WRONG NUMBER ENTERED \n");
    
	}  
	getch();
    return 0;
}
